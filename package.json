{
  "name": "trailblazer-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "pnpm run export:config",
    "dev": "vite dev",
    "dev:a3": "vite dev --mode a3",
    "build": "vite build",
    "preview": "vite preview",
    "test:pw": "playwright test",
    "test:unit": "vitest run --silent",
    "test:unit:debug": "vitest run",
    "test:unit:coverage": "vitest run --silent --coverage",
    "test:unit:watch": "vitest",
    "svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --ignore ./wagmi.config.ts ./src/generated",
    "svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --ignore ./wagmi.config.ts --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "pnpm format && eslint . --fix",
    "generate:abi": "cd ../taiko-mono.git/packages/nfts && pnpm compile && cd - && wagmi generate",
    "export:config": "pnpm ./scripts/exportJsonToEnv.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/auth-ui-svelte": "^0.2.9",
    "@supabase/supabase-js": "^2.43.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.9",
    "@types/debug": "^4.1.12",
    "@types/object-hash": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.4.0",
    "@wagmi/cli": "^2.1.4",
    "abitype": "^1.0.2",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "ethereum-address": "^0.0.4",
    "jsdom": "^24.0.0",
    "lokijs": "^1.5.12",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.17",
    "svelte-check": "^3.6.6",
    "tailwindcss": "^3.4.3",
    "ts-morph": "^19.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3",
    "vitest-fetch-mock": "^0.2.2",
    "vitest-mock-extended": "1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-vercel": "^5.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@wagmi/connectors": "^4.3.5",
    "@wagmi/core": "^2.9.3",
    "@walletconnect/ethereum-provider": "^2.11.3",
    "@walletconnect/modal": "^2.6.2",
    "@web3modal/wagmi": "^4.1.11",
    "@zerodevx/svelte-toast": "^0.9.5",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "ethers": "^6.13.0",
    "events": "^3.3.0",
    "object-hash": "^3.0.0",
