{
  "name": "trailblazer-ui",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "dev:prod": "vite dev --mode production",
    "dev:i18n-debug": "node scripts/i18nDebug.js && export PUBLIC_DEBUG_I18N=true && pnpm dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:pw": "playwright test",
    "test:unit": "vitest run --silent",
    "test:unit:debug": "vitest run",
    "test:unit:coverage": "vitest run --silent --coverage",
    "test:unit:watch": "vitest",
    "svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --ignore ./wagmi.config.ts --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "eslint . --fix",
    "codegen:abi": "cd ../taiko-mono/packages/nfts && pnpm compile && cd - && wagmi generate",
    "codegen:subgraph": "graphql-code-generator --config subgraph.codegen.ts"
  },
  "devDependencies": {
    "@apollo/client": "^3.11.4",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-operations": "^4.3.0",
    "@playwright/test": "^1.44.1",
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.10.1",
    "@types/object-hash": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vercel/analytics": "^1.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@wagmi/cli": "^2.1.8",
    "abitype": "^1.0.2",
    "ajv": "^8.16.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "ethereum-address": "^0.0.4",
    "graphql": "^16.9.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "jsdom": "^24.1.0",
    "lokijs": "^1.5.12",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-svelte": "^3.2.4",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "svelte-confetti": "^1.4.0",
    "tailwindcss": "^3.4.4",
    "ts-morph": "^19.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vitest-fetch-mock": "^0.2.2",
    "vitest-mock-extended": "1.3.1"
  },
  "type": "module",
  "dependencies": {
    "@ethereumjs/util": "^9.0.3",
    "@metamask/eth-sig-util": "^7.0.2",
    "@sveltejs/adapter-node": "^5.2.5",
    "@sveltejs/adapter-vercel": "^5.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@wagmi/connectors": "^5.0.9",
    "@wagmi/core": "^2.10.5",
    "@web3modal/wagmi": "^4.2.3",
    "@zerodevx/svelte-toast": "^0.9.5",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.13",
    "debug": "^4.3.5",
    "events": "^3.3.0",
    "graphql-codegen": "^0.4.0",
    "graphql-tag": "^2.12.6",
    "lefthook": "^1.7.12",
    "node-conditions": "^1.2.0",
    "object-hash": "^3.0.0",
    "postcss-import": "^16.1.0",
    "stores": "link:@zerodevx/svelte-toast/stores",
    "svelte-apollo": "^0.5.0",
    "svelte-countdown": "^1.1.2",
    "svelte-flip": "^0.0.11",
    "svelte-i18n": "^4.0.0",
    "viem": "^2.15.1"
  }
}
